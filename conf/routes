# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                                       module:play_base
#
# ~~~~

*		/?					  	                back.Application.index
*		/{action}								back.Application.{action}
*		/admin/{action}						    back.AdminController.{action}
*		/auth/{action}							back.AuthController.{action}
*		/api/{action}							back.ApiController.{action}
*		/job/{action}							back.JobController.{action}
*		/pro/{action}							back.ProController.{action}
*		/config/{action}						back.ConfigController.{action}


GET     /public/                                staticDir:public